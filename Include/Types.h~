#ifndef TYPES_H
#define TYPES_H

#include<stdio.h>
#include<time.h>
#include<stdlib.h>
#include<unistd.h>


//
//	typedefs
//
typedef long		_long;
typedef int		_int;
typedef char 		_byte;
typedef bool 		_bool;
typedef unsigned int 	_uint;
typedef unsigned long	_ulong;
typedef const char*	_string;

const _long sizeOfVector = 100000;


#endif
